{
  "name": "elbformat/sulu-behat-bundle",
  "description": "Context and Helpers to integrate behat tests in your sulu application",
  "type": "symfony-bundle",
  "license": "MIT",
  "keywords": [
    "sulu",
    "behat"
  ],
  "authors": [
    {
      "name": "Hannes Giesenow",
      "email": "hannes.giesenow@elbformat.de",
      "role": "Lead Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "Elbformat\\SuluBehatBundle\\": "src/"
    }
  },
  "require": {
    "php": ">=7.4",
    "ext-dom": "*",
    "ext-json": "*",
    "behat/behat": "^3.8",
    "friends-of-behat/symfony-extension": "^2.2",
    "sulu/sulu": "^2.4",
    "elbformat/symfony-behat-bundle": "^1.0"
  },
  "require-dev": {
    "dvdoug/behat-code-coverage": "5.2.2",
    "friendsofphp/php-cs-fixer": "3.11.0",
    "handcraftedinthealps/elasticsearch-bundle": "5.2.6.9",
    "jackalope/jackalope": "1.4.5",
    "jackalope/jackalope-doctrine-dbal": "1.8.0",
    "phpunit/phpunit": "9.5.25",
    "sulu/article-bundle": "2.4.0",
    "sulu/sulu-form-bundle": "2.5.0",
    "symfony/monolog-bundle": "^3.8",
    "symfony/runtime": "^5.4",
    "vimeo/psalm": "4.27.0"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "symfony/runtime": true,
      "composer/package-versions-deprecated": true
    }
  }
}
