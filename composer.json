{
  "name": "elbformat/sulu-behat-bundle",
  "description": "Context and Helpers to integrate behat tests in your sulu application",
  "type": "symfony-bundle",
  "license": "MIT",
  "keywords": [
    "sulu",
    "behat"
  ],
  "authors": [
    {
      "name": "Hannes Giesenow",
      "email": "hannes.giesenow@elbformat.de",
      "role": "Lead Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "Elbformat\\SuluBehatBundle\\": "src/"
    }
  },
  "require": {
    "php": ">=8.1",
    "ext-dom": "*",
    "ext-json": "*",
    "behat/behat": "^3.8",
    "elbformat/symfony-behat-bundle": "^1.5",
    "friends-of-behat/symfony-extension": "^2.2",
    "sulu/sulu": "^2.4",
    "symfony/event-dispatcher": "5.4.*"
  },
  "require-dev": {
    "dvdoug/behat-code-coverage": "^5.2",
    "friendsofphp/php-cs-fixer": "^3.3",
    "handcraftedinthealps/elasticsearch-bundle": "5.2.6.9",
    "jackalope/jackalope": "^1.4",
    "jackalope/jackalope-doctrine-dbal": "^1.7",
    "phpunit/phpunit": "^10.0",
    "sulu/article-bundle": "^2.3",
    "sulu/sulu-form-bundle": "^2.4",
    "symfony/mailer": "5.4.*",
    "symfony/monolog-bridge": "5.4.*",
    "symfony/monolog-bundle": "^3.8",
    "symfony/runtime": "5.4.*",
    "vimeo/psalm": "^4.13"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "symfony/runtime": true,
      "composer/package-versions-deprecated": true,
      "php-http/discovery": false
    }
  }
}
